{
  "name": "scout-dashboard",
  "version": "4.0.1",
  "description": "AI-powered analytics platform for Philippine sari-sari store interactions",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:synthetic": "node scripts/generateSyntheticData.js 50000 data/comprehensive_transactions.csv",
    "build:location": "node scripts/buildLocationDimension.js",
    "build:location-simple": "node scripts/buildLocationDimensionSimple.js",
    "deploy:supabase": "node scripts/deploy-to-supabase.js",
    "generate:data": "python scripts/fmcg-tobacco-generator.py",
    "load:data": "python scripts/postgres-data-loader.py comprehensive_fmcg_tobacco.csv",
    "setup:database": "npm run deploy:supabase && npm run generate:data"
  },
  "dependencies": {
    "@nivo/core": "^0.99.0",
    "@nivo/heatmap": "^0.99.0",
    "@nivo/sankey": "^0.99.0",
    "@supabase/supabase-js": "^2.39.3",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "lucide-react": "^0.300.0",
    "mapbox-gl": "^3.0.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "plotly.js": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^8.0.4",
    "react-plotly.js": "^2.6.0",
    "recharts": "^2.10.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-map-gl": "^6.1.7",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
