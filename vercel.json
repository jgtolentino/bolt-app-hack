{
  "env": {
    "VITE_OPENAI_API_KEY": "@openai_api_key",
    "VITE_OPENAI_API_BASE": "@openai_api_base",
    "VITE_OPENAI_MODEL": "@openai_model",
    "VITE_ANTHROPIC_API_KEY": "@anthropic_api_key",
    "VITE_ANTHROPIC_MODEL": "@anthropic_model"
  },
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    },
    {
      "source": "/geo/:file*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/geo/gadm41_PHL_1.json.gz",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "gzip"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}